varnishtest "VCL compiler coverage test: vcc_parse.c & vcc_expr.c"

varnish v1 -vcl {
	backend b { .host = "127.0.0.1"; }
	C{
	#include <stdio.h>
	}C
}

varnish v1 -badvcl { 0; }

varnish v1 -badvcl " sub vcl_recv { { } { "

varnish v1 -vcl {
	backend b { .host = "127.0.0.1"; }
	sub vcl_fetch {
		set beresp.ttl = 1w;
		set beresp.ttl *= 1.5;
		set beresp.ttl = 1.5 s * 2.5;
		set beresp.ttl = 1.5 s / 2.5;
		set beresp.ttl = 1.5h + 1.5s;
		set beresp.ttl = 1.5h - 1.5s;
	}
}

varnish v1 -badvcl {
	sub vcl_recv {
		if (!req.restarts != req.url) {
			set req.http.foo = "foo";
		}

	}
}

varnish v1 -badvcl {
	sub vcl_recv {
		set req.http.foo = vcl_recv;
	}
}

varnish v1 -badvcl {
	sub vcl_recv {
		if (req.grace < -3s || req.grace) {
			set req.http.foo = vcl_recv;
		}
	}
}

varnish v1 -badvcl {
	sub vcl_recv {
		if (req.grace < 3s && req.grace) {
			set req.http.foo = vcl_recv;
		}
	}
}

varnish v1 -badvcl {
	sub vcl_recv {
		set req.http.foo = "bla" * "foo";
	}
}

varnish v1 -badvcl {
	sub vcl_fetch {
		set req.http.foo = req.grace + beresp.status;
	}
}

varnish v1 -badvcl {
	sub vcl_fetch {
		if (! req.grace) {
		}
	}
}

varnish v1 -badvcl {
	sub vcl_fetch {
		if (beresp.do_gzip + beresp.do_gunzip) {
		}
	}
