varnish v1 -vcl {
	backend b { .host = "127.0.0.1"; }
	sub vcl_recv {
		set req.http.foo = "foo" + "bar";
		set req.http.foo = "foo" + 1;
		set req.http.foo = "foo" + now;

		set req.http.foo = now + 1s;
		set req.http.foo = now - 1s;
		set req.http.foo = now - now;

		set req.http.foo = 1 + 1;
		set req.http.foo = 1 - 1;

		set req.ttl = 1s;
	}
}

